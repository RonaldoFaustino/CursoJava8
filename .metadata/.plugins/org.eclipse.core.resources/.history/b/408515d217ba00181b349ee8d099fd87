package unidade1;

public class TexteExcecao {

	public static void main(String[] args) {
		try{
			int i = 50;
			//i = i/0;
			System.out.println("o resultado " + i );
			Object c = null;
			System.out.println("O resultado " + c.toString());
		}
		catch (ArithmeticException e)
		{
			e.printStackTrace();
		}
	}

}
