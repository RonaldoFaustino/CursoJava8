package unidade1;

public class TexteExcecao {

	public static void main(String[] args) throws ArithmeticException{
		try{
			int i = 50;
			//i = i/0;
			System.out.println(i);
			Object c = null;
			System.out.println(c.toString());
		}
		catch (ArithmeticException | NullPointerException e)
		{
			e.printStackTrace();
		}
	}

}
