package unidade3;

public class UnidadeDeExercicio {

	public static void main(String[] args) {
		String nome = "Curso de Java";
		String avaliacao = " é otimo";
		String data = "24/02/12";
		
		System.out.println(nome + avaliacao);

		String nome1= nome.substring(9,13);
		System.out.println(nome1);
		
		boolean valor = data.matches("\\d{2}/\\d{2}/\\d{4}");
        System.out.println("Data: "+valor);
		
}
        
	}


